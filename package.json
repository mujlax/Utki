{
  "name": "utki",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "vite",
    "dev:server": "tsx --env-file=.env.local src/server/index.ts",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "build": "tsc -b && vite build && npm run build:server",
    "build:server": "tsup src/server/index.ts --format esm --dts --out-dir dist/server",
    "lint": "eslint .",
    "preview": "vite preview",
    "start:server": "node dist/server/index.js",
    "test": "vitest run",
    "test:watch": "vitest watch --reporter=verbose",
    "seed": "tsx --env-file=.env.local scripts/dev-seed.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.3.1",
    "@mui/material": "^6.3.1",
    "@tanstack/react-query": "^5.62.9",
    "axios": "^1.7.9",
    "canvas-confetti": "^1.9.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.1",
    "googleapis": "^144.0.0",
    "react": "^19.1.1",
    "react-custom-roulette": "^1.4.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.5",
    "react-wheel-of-fortune": "^0.1.0",
    "uuid": "^11.0.3",
    "zod": "^3.24.1",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@ngrok/ngrok": "^1.5.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^2.1.6",
    "concurrently": "^9.1.0",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "jsdom": "^27.1.0",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "vitest": "^2.1.6"
  }
}
